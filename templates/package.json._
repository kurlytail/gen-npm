{
    "name": "@kurlytail/<%=design.MasterDesign[0].name%>",
    "description": "<%=design.MasterDesign[0].name%>",
    "version": "0.0.1",
    "main": "dist/app.min.js",
    "devDependencies": {
<%=extension('package-json-.*', 'devDependencies').filter(v => v.trim() != '').join(',')%>
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "license": "UNLICENSED",
    "dependencies": {
<%=extension('package-json-.*', 'dependencies').filter(v => v.trim() != '').join(',')%>
    },
<%=extension('package-json-.*').filter(v => v.trim() != '').join(',')%>
}