<%if (labels.includes('devDependencies')) {%>
        "jest": "^24.5.0",
        "jest-junit-reporter": "^1.1.0"
<%} else if (labels.length === 0) {%>
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src"
        ],
        "testPathIgnorePatterns": [
            "./.generated"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/**/*.js",
            "!**/node_modules/**"
        ],
        "setupFiles": [
            "jest-localstorage-mock"
        ]
    }
<%}%>